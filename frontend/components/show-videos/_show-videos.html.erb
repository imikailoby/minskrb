<section class="show-videos">
  <div class="show-videos-content">
    <ul class="show-videos-content__nav">
      <li><%= link_to "speakers", "#" %></li>
      <li><%= link_to "videos", "#" %></li>
      <li><%= link_to "photos", "#" %></li>
    </ul>

    <div class="show-videos-content__videos">
      <% @videos.link_id.each do |video| %>
        <div class="card">
          <iframe src="https://www.youtube.com/embed/<%= video %>?showinfo=0">
          </iframe>
          <p class="card__name">When the boobs whole world is your database (Sergey Sergyenko)</p>
        </div>
      <% end %>
    </div>
  </div>
</section>
