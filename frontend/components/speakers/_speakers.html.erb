<section class="speakers">
  <h3>Speakers_</h3>
  <div class="speakers-item">
    <% @event.event_items.each do |item| %>
      <% unless item.speaker_name.blank? %>
        <div class="speakers-item__card">
          <%= image_tag asset_pack_path('components/speakers/static/man.png') %>
          <p><%= item.speaker_name %></p>
        </div>
      <% end %>
    <% end %>
  </div>
</section>
