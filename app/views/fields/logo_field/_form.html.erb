<div class="field-unit__label">
  <%= f.label field.attribute %>
</div>
<div class="field-unit__field">
  <%= f.file_field field.attribute %>
  <%= image_tag field.x100 %>
</div>
<% if field.data.present? %>
  <div class="field-unit__nested">
    <%= link_to 'Remove', admin_event_media_partner_remove_logo_path(field.resource.id), method: :delete %>
  </div>
<% end %>
